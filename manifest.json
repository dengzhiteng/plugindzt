{
	"manifest_version": 1.0,
	"name": "谷歌插件",
	"version": "1.0",
	"description": "谷歌插件",
	"author": "邓志腾",
	"icons": {
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": ""
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content-script.js"
			],
			"run_at": "document_start"
		}
	],
	"homepage_url": "https://github.com/dengzhiteng",
	"web_accessible_resources": [
		"js/inject.js"
	]
}